dnl Process this file with autoconf to produce a configure script.
dnl
dnl $Id$

AC_PREREQ(2.13)
AC_INIT(i960ca)
RTEMS_TOP(../../../../..)
AC_CONFIG_AUX_DIR(../../../../..)

RTEMS_CANONICAL_TARGET_CPU

AM_INIT_AUTOMAKE(rtems-c-src-lib-libcpu-i960,$RTEMS_VERSION,no)
AM_MAINTAINER_MODE

RTEMS_ENABLE_BARE
RTEMS_ENV_RTEMSBSP

RTEMS_CHECK_CPU
RTEMS_CANONICAL_HOST

RTEMS_PROJECT_ROOT

RTEMS_PROG_CC_FOR_TARGET
RTEMS_CANONICALIZE_TOOLS

RTEMS_CHECK_CUSTOM_BSP(RTEMS_BSP)
RTEMS_CHECK_BSP_CACHE(RTEMS_BSP)

AM_CONDITIONAL(i960ca, test "$RTEMS_CPU_MODEL" = "i960ca")
AM_CONDITIONAL(i960ka, test "$RTEMS_CPU_MODEL" = "i960ka")
AM_CONDITIONAL(i960rp, test "$RTEMS_CPU_MODEL" = "i960rp")

# Explicitly list all Makefiles here
AC_OUTPUT( 
Makefile
i960ca/Makefile
i960ka/Makefile
i960rp/Makefile
include/Makefile)

