## LD = @LD@
AR = @AR@
RANLIB = @RANLIB@

ARFLAGS = ruv

define make-library
test -d $(ARCH) || mkdir $(ARCH)
$(RM) $@
$(AR) $(ARFLAGS) $@ $^
$(RANLIB) $@
endef

project_libdir = $(libdir)

$(PROJECT_RELEASE)/lib/$(dirstamp):
	@$(mkinstalldirs) $@
	@: > $(PROJECT_RELEASE)/lib/$(dirstamp)

TMPINSTALL_FILES = $(PROJECT_RELEASE)/lib

.PRECIOUS: $(LIB)
