AC_INIT(generic)
AC_CONFIG_AUX_DIR(../..)

RTEMS_TOP(../..)
RTEMS_CANONICAL_TARGET_CPU

AM_INIT_AUTOMAKE(rtems-cpu-tools,$RTEMS_VERSION,no)
